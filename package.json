{
  "name": "wemake-vue-template",
  "version": "0.1.0",
  "description": "My Superb Vue Project",
  "main": "client/index.js",
  "private": true,
  "engines": {
    "node": "^8.8.0",
    "npm": "^5.4.2"
  },
  "scripts": {
    "test:unit": "jest",
    "test": "npm run test:unit",
    "start": "cross-env NODE_ENV=production node server",
    "clean": "rimraf dist",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "dev": "node build/dev-server.js",
    "lint": "eslint test build client --ext .js --ext .vue"
  },
  "author": "sobolevn <mail@sobolevn.me>",
  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "collectCoverage": true,
    "coverageFormats": ["json", "html"],
    "verbose": true,
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "ajv": "^5.3.0",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-vue-app": "^1.1.1",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.13.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "vue-jest": "^1.0.0",
    "vue-loader": "^11.0.0",
    "vue-server-renderer": "^2.5.2",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.2.0",
    "vue-test-utils": "^1.0.0-beta.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.0"
  }
}
